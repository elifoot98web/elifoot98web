cd D:\ELI98\PATCH

IF NOT EXIST FLAGS GOTO EQUIPAS
DEL ..\FLAGS\*.*
COPY FLAGS\*.* ..\FLAGS
ECHO "PATCHED FLAGS"

:EQUIPAS
IF NOT EXIST EQUIPAS GOTO COUNTRY
DEL ..\EQUIPAS\*.*
COPY EQUIPAS\*.* ..\EQUIPAS
ECHO "PATCHED EQUIPAS"

:COUNTRY
IF NOT EXIST COUNTRY.TXE GOTO PLOP
DEL ..\COUNTRY.TXE
COPY COUNTRY.TXE ..\COUNTRY.TXE
ECHO "PATCHED COUNTRY.TXE"

:PLOP
IF NOT EXIST CTRGROUP GOTO PATCHEND

cd CTRGROUP
IF NOT EXIST PLOP.TXE GOTO BOSMAN
DEL ..\..\CTRGROUP\PLOP.TXE
COPY PLOP.TXE ..\..\CTRGROUP\PLOP.TXE
ECHO "PATCHED CTRGROUP\PLOP.TXE"

:BOSMAN
IF NOT EXIST BOSMAN.TXE GOTO PATCHEND
DEL ..\..\CTRGROUP\BOSMAN.TXE
COPY BOSMAN.TXE ..\..\CTRGROUP\BOSMAN.TXE
ECHO "PATCHED CTRGROUP\BOSMAN.TXE"

:PATCHEND
cd D:\ELI98

ECHO "PATCH_END"